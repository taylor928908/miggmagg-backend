{
  "type": "object",
  "properties": {
    "phone": {
      "type": "string",
      "pattern": "^01([016789]?)([0-9]{3,4})([0-9]{4})$"
    },
    "phoneToken": {
      "type": "string"
    },
    "password": {
      "type": "string",
      "pattern": "^[0-9a-zA-Z]{8,14}$"
    }
  },
  "required": [
    "phone",
    "phoneToken",
    "password"
  ],
  "additionalProperties": false
}
